    "editor",
    "glob",
    "graceful-fs",
    "hosted-git-info",
    "inherits",
    "ini",
    "init-package-json",
    "is-cidr",
    "leven",
    "libnpmaccess",
    "libnpmfund",
    "libnpmhook",
    "libnpmorg",
    "libnpmpack",
    "libnpmpublish",
    "libnpmsearch",
    "libnpmteam",
    "libnpmversion",
    "make-fetch-happen",
    "mkdirp",
    "mkdirp-infer-owner",
    "ms",
    "nopt",
    "npm-audit-report",
    "npm-package-arg",
    "npm-pick-manifest",
    "npm-profile",
    "npm-registry-fetch",
    "npm-user-validate",
    "npmlog",
    "opener",
    "pacote",
    "parse-conflict-json",
    "qrcode-terminal",
    "read",
